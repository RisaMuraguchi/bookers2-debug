<% if user_signed_in? %>
  <%= form_with url: search_path, method: :get, local: true do |f| %>
    <%= f.text_field :content, class: "form-control form-control-sm", style: "width: 300px; height: 5px;" %>
    <div class="form-group">
    <%= f.select :model, options_for_select({ "User" => "user", "Book" => "book" }), {}, {class: 'form-control', style: "width: 300px; height: 40px;"} %>
    </div>
    <%= f.select :method, options_for_select({ "完全一致" => "perfect", "前方一致" => "forward", "後方一致" => "backward", "部分一致" => "partial" }) %>
    <%= f.submit '検索' %>
  <% end %>
<% end %>